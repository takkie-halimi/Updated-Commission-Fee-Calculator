{
  "name": "payme/commission-fee-calculator",
  "authors": [
    {
      "name": "Takkie Eddine Halimi",
      "email": "takkie8halimi@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Payme\\CommissionFeeCalculator\\": "src"
    }
  },
    "autoload-dev": {
      "psr-4": {
        "Payme\\CommissionFeeCalculator\\Tests\\": "tests"
      }
    },
    "require": {
      "league/csv": "^9.1",
      "ext-bcmath": "*",
      "ext-curl": "*"
    },
    "require-dev": {
      "phpunit/phpunit": "^7",
      "squizlabs/php_codesniffer": "3.*",
      "opsway/psr12-strict-coding-standard": "^0.7.0"
    },
    "scripts": {
      "cs-check": "phpcs",
      "cs-fix": "phpcbf"
    },
    "config": {
      "allow-plugins": {
        "dealerdirect/phpcodesniffer-composer-installer": true
      }
    }
}
